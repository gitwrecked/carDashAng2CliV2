{
  "name": "car-app",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "scripts": {
    "heroku-prebuild": "npm install -g angular-cli",
    "heroku-postbuild": "ng build",
    "start": "node server/server.js",
    "watch": "ng build --watch",
    "lint": "tslint \"client/**/*.ts\"",
    "test": "ng test",
    "test-api": "jasmine-node server/api/test/",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "seed-users": "mongoimport -h localhost:27017 -d test -c users --file server/seed/users.json",
    "seed-purchases": "mongoimport -h localhost:27017 -d test -c purchases --file server/seed/purchases.json",
    "seed-db": "npm run-script seed-users ; npm run-script seed-purchases",
    "format-ts": "clang-format -i -style=file --glob=client/**/*.ts",
    "format-js": "clang-format -i -style=file --glob=server/**/*.js",
    "format-html": "clang-format -i -style=file --glob=client/**/*.html",
    "format": "npm run format-ts ; npm run format-js ; npm run format-html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "angular2-jwt": "^0.1.27",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.15.2",
    "chart.js": "2.4.0",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "express": "4.14.0",
    "jsonwebtoken": "7.2.1",
    "mongodb": "2.2.11",
    "mongoose": "4.7.0",
    "morgan": "1.7.0",
    "ng2-charts": "1.4.1",
    "rootpath": "0.1.2",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "2.3.2",
    "ts-helpers": "1.1.1",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.42",
    "angular-cli": "1.0.0-beta.21",
    "clang-format": "1.0.46",
    "codelyzer": "1.0.0-beta.3",
    "frisby": "0.8.5",
    "jasmine-core": "2.5.2",
    "jasmine-node": "1.14.5",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
